- if controller_name != 'sessions'
  .signBox__link
    = link_to "ログイン", new_session_path(resource_name), class: 'btn'
    %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  .signBox__link
    = link_to "新規登録", new_registration_path(resource_name), class: 'btn'
    %br/
